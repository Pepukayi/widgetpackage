{
    "name": "pesto/widget",
    "description": ":package_description",
    "authors": [
        {
            "name": "David Chitondo",
            "email": "david.chitondo@payfast.co.za",
            "homepage": "https://payfast.co.za"
        }
    ],
    "homepage": "https://github.com/pesto/widget",
    "keywords": ["Laravel", "Widget"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Pesto\\Widget\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pesto\\Widget\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pesto\\Widget\\WidgetServiceProvider"
            ],
            "aliases": {
                "Widget": "Pesto\\Widget\\Facades\\Widget"
            }
        }
    }
}
